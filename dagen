x = ['maandag', 'dinsdag', 'woensdag', 'donderdag', 'vrijdag', 'zaterdag', 'zondag']

for i in x:
    print(i)

for o in range(5):
    print(x[o])

for y in range(5,7):
    print(x[y])

for t in reversed(x):
    print(t)

for u in range(5):
    print(x[u::-1])

for h in range(1):
    x.reverse()
    print(x[0:2])
