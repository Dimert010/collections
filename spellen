from random import choice
from random import randint

game_list = ['Monopoly', 'Yathzee', 'Bridge', 'Poker', 'Pesten', 'Mens erger je niet', 'Cluedo']

def spelProgramma(spelList, minimale, maximale):
    gamePlay = []
    for  u in range(randint(minimale, maximale)):
        gamePlay.append (choice(spelList))
    return gamePlay

print(spelProgramma(game_list, 1, 10))
